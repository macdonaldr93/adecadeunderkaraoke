{
  "name": "adecadeunderkaraoke",
  "version": "1.0.0",
  "scripts": {
    "build:css:main": "postcss css/main.css -o css/main.min.css",
    "build:css:vendor": "postcss css/normalize.css -o css/normalize.min.css",
    "build:js:main": "minify js/main.js > js/main.min.js",
    "build:js:vendor": "minify js/vendor.js > js/vendor.min.js",
    "build": "npm run build:css:main && npm run build:css:vendor && npm run build:js:main && npm run build:js:vendor"
  },
  "author": "Ryan Macdonald",
  "license": "MIT",
  "dependencies": {
    "minify": "12.0.0"
  },
  "devDependencies": {
    "autoprefixer": "10.4.20",
    "cssnano": "7.0.6",
    "postcss": "8.4.49",
    "postcss-cli": "11.0.0"
  }
}
